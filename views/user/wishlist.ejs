    <!------ Include the above in your HEAD tag ---------->
   
    <% if(wishData.wishlist[0]!==undefined){ %>
        <div class="cart-wrap" style="margin-top: 100px; width: 1900px;">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="main-heading mb-10">My wishlist</div>
                        <div class="table-wishlist">
                            <table cellpadding="0" cellspacing="0" border="0" width="100%">
                                <thead>
                                    <tr>
                                        <th width="45%">Product Name</th>
                                        <th width="15%">Unit Price</th>
                                        <th width="15%">Stock Status</th>
                                        <th width="15%"></th>
                                        <th width="10%"></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% let s =0 %>
                                <% for(let i =0 ; i<wishData.wishlist.length;i++) {%>
                                
                       
                               
                                 <% if (wishData.wishlist[i].is_in==0) { %>
                                
                                    <tr>
                                        <td width="45%">
                                            <div class="display-flex align-center">
                                                <div class="img-product">
                                                    <img src=" prodectImages/<%=wishData.wishlist[i].image %> " alt="<%= wishData.wishlist[i].image%> "> </div>
                                                <div class="name-product">
                                                    <%=wishData.wishlist[i].name%>
                                                </div>
                                            </div>
                                        </td>
                                        <td width="15%" class="price"><%=wishData.wishlist[i].Price%></td>

                                       <% if (wishData.wishlist[i].quantity==0) {%>
                                        <td width="15%"><span class="in-stock-box">OUT OF Stock</span></td>
                                        <td width="15%"><a href="#"><button onclick="wishlist()" class="round-black-btn small-btn bg-success">Add to Cart</button></a></td>
                                        <td width="15%"><<a href="/removeWishlist?id=<%=wishData.wishlist[i].productId %>"><button class="round-black-btn small-btn">remove</button></a></td>
                                      <% }else{ %>
                                        <td width="15%"><span class="in-stock-box"style="background-color:green;">In Stock</span></td>
                                        <td width="15%"><a onclick="move('<%=wishData.wishlist[i].productId %>')"><button  class="round-black-btn small-btn bg-success">Add to Cart</button></a></td>
                                        <td width="15%"><a href="/removeWishlist?id=<%=wishData.wishlist[i].productId %>"><button class="round-black-btn small-btn">remove</button></a></td>
                                         <% } %>
                                       
                                        </a></td>
                                    </tr>
                                    <%}%>
                             
                                 
                                 <% } %>

                              
                       
                             
                                  
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <% }else{ %>
        
            <div class="container" style="margin-top: 200px;">
                <div class="row">
                    <div class="col-md-12">
                        <div class="error-template">
                            <h1>
                                Oops!</h1>
                            <h2>
                                PLEASE DO ADD SOMTHING TO THE WISHLIST </h2>
                            <div class="error-details">
