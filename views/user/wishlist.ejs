                        </div>
                    </div>
                </div>
            </div>

        <%}%>
       <script>

        function wishlist () {
            alert('the orderd product is out of stock  ')
        }

        const move= function(id) {
   console.log('it is working----#');
  
   $.ajax({
       url: '/cartAdd',
       method: 'PATCH',
       dataType: 'json',
       data: {
           id: id
       }
   }).done(data => {
       if (data.success==true) {
        Swal.fire(
         'Good job!',
           'Your order is added to cart successfully',
           'success'
) 
setTimeout(() => {
    location.reload(); 
}, 2000);


       }
   })
}

