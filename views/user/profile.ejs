<div id="myModal" class="modal10">
    <div class="modal-content"style="width:30%">
      <span class="close">&times;</span>
      <h3 style="display: flex;justify-content: center;">CHANGE PASSWORD</h3>
     <form id="formPassword" >
  <i>
      <h6 id="message" style="color: #ff3368; text-align: center;"></h6>
  </i>
      <div id="mainFeild" class="mb-3">
          <label for="message-text" class="col-form-label">E-MAIL:</label>
          <input name="email" type="text" class="form-control" id="recipient-name">
        </div>
        <div  id="mainFeild1" class="mb-3">
          <label for="message-text" class="col-form-label">CURRENT PASSWORD:</label>
          <input name="password" type="number" class="form-control" id="recipient-name">
        </div>
        <div id="hiddenFeilds" hidden class="mb-3">
          <label  for="message-text" class="col-form-label">NEW PASSWORD:</label>
          <input name="newPassword" type="number" class="form-control" id="recipient-name">
        </div>
        <div id="hiddenFeilds1" hidden class="mb-3">
          <label  for="message-text" class="col-form-label">CONFIRM PASSWORD:</label>
          <input name="confirmPassword" type="number" class="form-control" id="recipient-name">
        </div>
        <button  id="satbutton"  type="submit" class="btn btn-dark" style="display: flex;justify-content: center;">SUBMIT </button>
        <button id="satbutton1"hidden  type="submit" class="btn btn-success" style="display: flex;justify-content: center;">SET PASSWORD </button>
  
     </form>
    </div>
  </div>
  
  
  <style>
  
  .modal10 {
      width: 500px;
    display: none;
    position: fixed;
    z-index: 1;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    overflow: auto;
    background-color: rgba(0,0,0,0.4);
  }
  
  .modal-content {
    background-color: #fff;
    margin: 15% auto;
    padding: 20px;
    border: 1px solid #888;
    width: 20%;
    height: 350px;
  }
  
  .close {
    color: #aaa;
    float: right;
    font-size: 28px;
    font-weight: bold;
  }
  
  .close:hover,
  .close:focus {
    color: black;
    text-decoration: none;
    cursor: pointer;
  }
  
  </style>
  <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

  <script>
  
  
      const modal = document.getElementById("myModal");
  const btn = document.getElementById("myBtn");
  const span = document.getElementsByClassName("close")[0];
  
  btn.onclick = function() {
    modal.style.display = "block";
  }
  
  span.onclick = function() {
    modal.style.display = "none";
  }
  
  window.onclick = function(event) {
    if (event.target == modal) {
      modal.style.display = "none";
    }
  }
  
 
  
  </script>
  <script>
    $('#formPassword').submit((e)=>{
        e.preventDefault()
        
        $.ajax({
            url: '/passwordChange',
            data: $('#formPassword').serialize(),
            method: 'post',
            success: (response) => {
                console.log('Data successfully coming ---------------');
                if (response.value==1) {
                    $("#message").html(response.message);
                       
                }else if(response.value==2){
                    $("#hiddenFeilds").removeAttr('hidden');
                    $("#hiddenFeilds1").removeAttr('hidden');
                    $("#mainFeild").hide();
                    $("#mainFeild1").hide();
                    $("#satbutton").hide();
                    $("#satbutton1").removeAttr('hidden');
                }else if(response.value==3){
                    $("#message").html(response.message);   
    
                }else if(response.value==4){
                    Swal.fire(
                            ' success!',
                            'your new password is added!',
                            'success'
                            )
                            setTimeout(() => {
                                window.location = "/profile"; 
                            }, 1000);
                            let message = "ORDER SUCCESSFUL"
                          
                            $("#message").html(message); 
                }else if(response.value==5){
                    
                        $("#hiddenFeilds").removeAttr('hidden');
                    $("#hiddenFeilds1").removeAttr('hidden');
                    $("#mainFeild").hide();
                    $("#mainFeild1").hide();
                    $("#satbutton").hide();
                    $("#satbutton1").removeAttr('hidden');
                    $("#message").html(response.message); 
                }else if(response.value==6){
                    
                    $("#hiddenFeilds").removeAttr('hidden');
                $("#hiddenFeilds1").removeAttr('hidden');
                $("#mainFeild").hide();
                $("#mainFeild1").hide();
                $("#satbutton").hide();
                $("#satbutton1").removeAttr('hidden');
                $("#message").html(response.message); 
            }
            }
        });
    });
    console.log('value is comming trueeeee------');
                  
    </script>
