<style>
  * {
  box-sizing: border-box;
}

body {
  font-family: Arial, sans-serif;
}

.history-container {
  padding: 30px;
  border: 1px solid gray;
  box-shadow: 0 1px 3px 0 rgba(0,0,0,0.45);
}

.history-container > .header {
  margin-bottom: 30px;
}

h2 {
  font-size: 24px;
  margin: 0;
  font-weight: normal;
}

.account-balance {
  display: flex;
  flex-grow: 1;
  align-items: baseline;
  margin-top: 15px;
}

.account-balance > .label {
  font-size: 14px;
}

.account-balance > amount {
  font-size: 16px;
}

.dots {
  border-bottom: 1px dotted #ccc;
  flex-grow: 1;
}

.transactions {
  background-color: #ededed;
  margin-top: 30px;
  margin-bottom: 10px;
  padding: 20px;
}

.transactions > table {
  width: 100%;
  border-collapse: collapse;
  border-bottom: 1px solid #ccc;
}

.transactions > table td {
  padding: 0;
}

h3 {
  font-size: 18px;
  margin: 0;
  font-weight: normal;
  padding: 10px 0;
}

.transaction-row {
  height: 30px;
  white-space: pre;
}

.transaction-date {
  width: 23%; /*?*/
}

.transaction-amount {
  text-align: right;
}
</style>
<div class="history-container" style="margin: 0px;">
  <div class="header">
    <h2>WALLET -HISTORY</h2>
  </div>
  <div class="header">
    <h2>user name_:<%=usersData.name%></h2>
  </div>
  <div>
    <div class="balances">
      <div class="account-balance">
        <span class="label">Current</span>
        <span class="dots"></span>
        <span class="amount">$<%=usersData.wallet%></span>
      </div>
      <div class="account-balance">
        <span class="label">Available</span>
        <span class="dots"></span>
        <span class="amount">$<%=usersData.wallet%></span>
      </div>
    </div>
    <div class="transactions">
      <h3>WALLET INCOMING PRICE HISTORY</h3>
      <table>
     
        <tbody>
          <tr class="transaction-row" style="color: black;font-weight: bold;">
            <td class="transaction-date">PURCHASE DATE</td>
            <td>PRODUCT COUNT</td>
            <td>$PURCHASE AMOUNT</td>
            <td>ORDER TYPE</td>
            <td class="transaction-amount">TOTAL PRICE</td>

          </tr>
          <% let halo=0 %>
         
          <% for(let i = 0; i < orders.length; i++) { %>
            <% if (orders[i].orderType=='Razorpay') { %>
              <% halo= orders[i].totalPrice +halo%>
           
              <tr class="transaction-row">
                <td class="transaction-date"><%= orders[i].date.toLocaleDateString() %></td>
                <td><%= orders[i].products.length %></td>
                <td><%= orders[i].totalPrice %></td>
                <td>ROZARPAY </td>
                  <td class="transaction-amount">$<%=halo%></td>
              </tr>
            <% } %>
          <% } %>

        </tbody>
      </table>
    </div>
    <div class="transactions">
      <h3>WALLET INCOMING PRICE HISTORY OF RETUREND ORDER</h3>
      <table>
     
        <tbody>
          <tr class="transaction-row" style="color: black;font-weight: bold;">
            <td class="transaction-date">PURCHASE DATE</td>
            <td>PRODUCT COUNT</td>
            <td>$PURCHASE AMOUNT</td>
            <td>ORDER TYPE</td>
            <td class="transaction-amount">TOTAL PRICE</td>

          </tr>
    
         <% let values=0 %>
          <% for(let i = 0; i < returned.length; i++) { %>
         
              <% values= returned[i].totalPrice +values%>
           
              <tr class="transaction-row">
                <td class="transaction-date"><%= returned[i].date.toLocaleDateString() %></td>
                <td><%= returned[i].products.length %></td>
                <td><%= returned[i].totalPrice %></td>
                <td>returned </td>
                  <td class="transaction-amount">$<%=values%></td>
              </tr>
          <% } %>

        </tbody>
      </table>
    </div>
    <div class="transactions">
      <h3>WALLET PAYMENTS</h3>
      <table>

        <tbody>
          <tr class="transaction-row" style="color: black;font-weight: bold;">
            <td class="transaction-date">PURCHASE DATE</td>
            <td>PRODUCT COUNT</td>
            <td>$PURCHASE AMOUNT</td>
            <td>ORDER TYPE</td>
            <td class="transaction-amount">TOTAL PRICE</td>

          </tr>
          <% let val=0 %>
          <% for(let i = 0; i < orders.length; i++) { %>
            <% if (orders[i].orderType=='walletPayment') { %>
              <% val= orders[i].totalPrice +val%>
              <tr class="transaction-row">
                <td class="transaction-date"><%= orders[i].date.toLocaleDateString() %></td>
                <td><%= orders[i].products.length %></td>
                <td><%= orders[i].totalPrice %></td>
                <td>WALLET </td>
                  <td class="transaction-amount">$<%=val%></td>
              </tr>
            <% } %>
          <% } %>
         
        </tbody>
      </table>
    </div>
    <div class="transactions">
      <h3>TOTTAL AMOUNT OVEROLL USED</h3>
      <% let m=0 %>
      <% for(let i = 0; i < orders.length; i++) { %>
        <% if (orders[i].orderType=='walletPayment'||orders[i].orderType=='Razorpay') { %>
          <% m= orders[i].totalPrice +m%>
    
        <% } %>
      <% } %>
       <h3>TOTAL:-------------------------------:<%=m%></h3>
    </div>
  </div>
  <div class="footer">
  </div>
</div>
